					 ArrivalDateTime:到达时间，FlightNumber：航班号
				-->
        <FlightSegment Status="HK" DepartureDateTime="2012-09-25T07:30:00" ArrivalDateTime="2012-09-25T09:30:00" FlightNumber="CA1831">
          <!-- 出发城市 -->
          <DepartureAirport LocationCode="PEK"/>
          <!-- 到达城市 -->
          <ArrivalAirport LocationCode="SHA"/>
