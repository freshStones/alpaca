          <Equipment AirEquipType="773" />
          <!-- 舱位 -->
          <BookingClassAvail ResBookDesigCode="Y" />
        </FlightSegment>
        <!-- Status:行动代码， DepartureDateTime：出发时间（注意格式，此格式方便后面转换）
					 ArrivalDateTime:到达时间，FlightNumber：航班号
				-->
        <FlightSegment Status="HK" DepartureDateTime="2012-09-26T07:30:00" ArrivalDateTime="2012-09-26T09:30:00" FlightNumber="CA1590">
          <!-- 出发城市 -->
          <DepartureAirport LocationCode="SHA"/>
          <!-- 到达城市 -->
          <ArrivalAirport LocationCode="PEK"/>
          <!-- 机型 -->
          <Equipment AirEquipType="773" />
          <!-- 舱位 -->
          <BookingClassAvail ResBookDesigCode="Y" />
        </FlightSegment>

      </OriginDestinationOption>
    </OriginDestinationOptions>
  </AirItinerary>
  <TravelerInfoSummary>
    <AirTravelerAvail>
      <!-- 旅客类型 -->
