<OTA_QMsgRQ>
<!--操作类型，OPQT,OPQR ,OPQX-->
	<ActionCode>OPQX</ActionCode>
	<!--是否删除整个RSN标记，0 – 否，1 – 是（此时不需要输入QueueInfo节点，需要添加Agent节点），默认：0（此时不需要Agent，只需要QueueInfo节点）-->
	<XRSNFlag>0</XRSNFlag>
	<!--信箱类型 SC office号 BJS191-->
	<Queue Office="BJS191" QueueType="SC">
	<!--XRSNFlag=0，删除具体的Q信息可以多个  开始-->
		<QueueInfo>
			<ID>1829937091</ID>
		</QueueInfo>
		<QueueInfo>
			<ID>1830849401</ID>
		</QueueInfo>
		<QueueInfo>
			<ID>1830849462</ID>
		</QueueInfo>
	<!--XRSNFlag=0，删除具体的Q信息可以多个  结束-->
	
	<!--XRSNFlag=1，删除某个Agent号下的Q信息  开始-->
		<Agent>8888</Agent>
		<Agent>78849</Agent>
	<!--XRSNFlag=1，删除某个Agent号下的Q信息  结束-->
	</Queue>
</OTA_QMsgRQ>

